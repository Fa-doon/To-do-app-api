<head>
  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    nav {
      background-color: #282147;
      color: whitesmoke;
      height: 70px;
      width: 100%;
      font-family: sans-serif;
      font-size: 18px;
    }

    nav ul {
      display: flex;
      justify-content: center;
      align-items: center;
    }

    nav ul li {
      margin-right: 80px;
      line-height: 4;
      list-style-type: none;
    }

    nav ul li a {
      text-decoration: none;
      color: whitesmoke;
    }
  </style>
</head>
<nav>
  <ul>
    <% navs.forEach(nav =>{ %>
    <li><a href="/<%=nav%>"><%=nav%></a></li>
    <% }) %>
  </ul>
</nav>
